{
  "name": "cottontail-client",
  "version": "0.1.0",
  "scripts": {
    "rimraf": "node_modules/rimraf/bin.js",
    "tslint": "node_modules/tslint/bin/tslint",
    "typedoc": "node_modules/typedoc/bin/typedoc",
    "typings": "node_modules/typings/dist/bin.js",
    "webpack": "node_modules/webpack/bin/webpack.js",
    "webpack-dev-server": "node_modules/webpack-dev-server/bin/webpack-dev-server.js",
    "webdriver-manager": "node_modules/webdriver-manager/bin/webdriver-manager",
    "protractor": "node_modules/protractor/bin/protractor",
    "clean": "npm cache clean && npm run rimraf -- node_modules doc typings coverage dist",
    "clean:dist": "npm run rimraf -- dist",
    "preclean:install": "npm run clean",
    "clean:install": "npm set progress=false && npm install",
    "preclean:start": "npm run clean",
    "clean:start": "npm start",
    "watch": "npm run watch:dev",
    "watch:dev": "npm run build:dev -- --watch",
    "watch:dev:hmr": "npm run watch:dev -- --hot",
    "watch:test": "npm run test -- --auto-watch --no-single-run",
    "watch:prod": "npm run build:prod -- --watch",
    "build": "npm run build:dev",
    "prebuild:dev": "npm run clean:dist",
    "build:dev": "webpack --config config/webpack.dev.js --progress --profile --colors --display-error-details --display-cached",
    "prebuild:prod": "npm run clean:dist",
    "build:prod": "webpack --config config/webpack.prod.js  --progress --profile --colors --display-error-details --display-cached --bail",
    "server": "npm run server:dev",
    "server:dev": "webpack-dev-server --config config/webpack.dev.js --inline --progress --profile --colors --watch --display-error-details --display-cached --content-base src/",
    "server:dev:hmr": "npm run server:dev -- --hot",
    "server:prod": "http-server dist --cors",
    "webdriver:update": "npm run webdriver-manager update",
    "webdriver:start": "npm run webdriver-manager start",
    "lint": "npm run tslint 'src/**/*.ts'",
    "pree2e": "npm run webdriver:update -- --standalone",
    "e2e": "npm run protractor",
    "e2e:live": "npm run e2e -- --elementExplorer",
    "test": "node --max-old-space-size=4096 node_modules/karma/bin/karma start",
    "ci": "npm run e2e && npm run test",
    "docs": "npm run typedoc -- --options typedoc.json --mode file --exclude '**/*.spec.ts' ./src/",
    "start": "npm run server:dev",
    "start:hmr": "npm run server:dev:hmr",
    "postinstall": "npm run typings -- install",
    "preversion": "npm test",
    "version": "npm run build",
    "postversion": "git push && git push --tags",
    "debug:test": "npm run watch:test -- --debug"
  },
  "dependencies": {
    "@angular/common":  "2.0.0-rc.3",
    "@angular/compiler":  "2.0.0-rc.3",
    "@angular/core":  "2.0.0-rc.3",
    "@angular/forms": "0.1.1",
    "@angular/http":  "2.0.0-rc.3",
    "@angular/platform-browser":  "2.0.0-rc.3",
    "@angular/platform-browser-dynamic":  "2.0.0-rc.3",
    "@ngrx/core": "^1.0.0",
    "@ngrx/effects": "^1.0.1",
    "@ngrx/store": "^2.0.0",
    "brace": "^0.8.0",
    "core-js": "^2.4.0",
    "font-awesome": "^4.6.1",
    "golden-layout": "deepstreamIO/golden-layout#master",
    "jquery": "^2.2.3",
    "lodash": "^4.13.0",
    "normalize.css": "^4.1.1",
    "q": "^1.4.1",
    "resolve-url-loader": "^1.4.3",
    "rxjs": "5.0.0-beta.6",
    "socket.io": "^1.4.6",
    "socket.io-client": "^1.4.6",
    "zone.js": "^0.6.12",
      "reflect-metadata": "0.1.3"
  },
  "devDependencies": {
    "angular2-hmr": "^0.7.0",
    "awesome-typescript-loader": "^0.16.2",
    "bootstrap": "^4.0.0-alpha",
    "codelyzer": "0.0.15",
    "compression-webpack-plugin": "^0.3.1",
    "copy-webpack-plugin": "^3.0.0",
    "css-loader": "^0.23.1",
    "cwlts": "^1.2.0",
    "es6-promise": "^3.1.2",
    "es6-promise-loader": "^1.0.1",
    "es6-shim": "^0.35.0",
    "es7-reflect-metadata": "^1.6.0",
    "exports-loader": "^0.6.3",
    "expose-loader": "^0.7.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "html-webpack-plugin": "^2.15.0",
    "http-server": "^0.9.0",
    "imports-loader": "^0.6.5",
    "install": "^0.8.1",
    "istanbul-instrumenter-loader": "^0.2.0",
    "json-loader": "^0.5.4",
    "jsonschema": "^1.1.0",
    "karma": "^0.13.22",
    "karma-chrome-launcher": "^0.2.3",
    "karma-coverage": "^0.5.5",
    "karma-jasmine": "^0.3.8",
    "karma-mocha-reporter": "^2.0.0",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-remap-istanbul": "^0.1.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "1.7.0",
    "node-sass": "^3.5.3",
    "npm": "^3.10.3",
    "parse5": "^2.1.5",
    "phantomjs-polyfill": "0.0.2",
    "phantomjs-prebuilt": "^2.1.7",
    "protractor": "^3.2.2",
    "raw-loader": "0.5.1",
    "remap-istanbul": "^0.5.1",
    "rimraf": "^2.5.2",
    "sass-loader": "^3.2.0",
    "source-map-inline-loader": "blackbaud-bobbyearl/source-map-inline-loader",
    "source-map-loader": "^0.1.5",
    "source-map-support": "^0.4.0",
    "style-loader": "^0.13.1",
    "ts-helpers": "1.1.0",
    "ts-node": "^0.7.1",
    "tslint": "^3.7.4",
    "tslint-loader": "^2.1.3",
    "typedoc": "^0.3.12",
    "typescript": "^1.8.10",
    "typings": "^0.7.12",
    "url-loader": "^0.5.7",
    "webdriver-manager": "^9.0.0",
    "webpack": "^1.13.0",
    "webpack-dev-server": "^1.14.1",
    "webpack-md5-hash": "^0.0.5",
    "webpack-merge": "^0.13.0",
    "worker-loader": "^0.7.0"
  },
  "engines": {
    "node": ">= 4.2.1",
    "npm": ">= 3"
  }
}
