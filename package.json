{
    "name": "rabix-composer",
    "version": "1.0.0-rc.1",
    "license": "Apache-2.0",
    "homepage": "https://github.com/rabix/composer#readme",
    "description": "Rabix Composer is a desktop IDE for working with Common Workflow Language workflows",
    "angular-cli": {},
    "author": "Seven Bridges",
    "bugs": {
        "url": "https://github.com/rabix/composer/issues"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/rabix/composer.git"
    },
    "scripts": {
        "build": "yar run compile:electron && yar run compile:angular && yar run package",
        "changelog": "./node_modules/.bin/git-changelog",
        "compile:angular": "./node_modules/.bin/ng build -prod -aot",
        "compile:electron": "./node_modules/.bin/tsc -p electron && cp -R electron/src/splash electron/dist/src",
        "compile:electron:watch": "./node_modules/.bin/tsc -p electron --watch",
        "lint": "./node_modules/.bin/ng lint",
        "package": "node ./scripts/electron-build.js",
        "postinstall": "cd electron && yar install",
        "serve": "./node_modules/.bin/ng serve",
        "start:electron": "./node_modules/.bin/electron ./electron",
        "test": "yar run test:electron && yar run test:angular && yar run test:spectron",
        "test:angular": "./node_modules/.bin/ng test --watch false",
        "test:angular:watch": "./node_modules/.bin/ng test",
        "test:spectron": "node ./node_modules/.bin/_mocha -r ts-node/register --bail --ui bdd ./spectron/**/*.func.ts",
        "test:electron": "./node_modules/electron/cli.js ./electron/node_modules/.bin/_mocha -r ts-node/register --recursive --ui bdd 'electron/**/*.spec.ts'",
        "ci": "yar run build && yar run test:spectron && yar run test:electron && npm run test:angular",
        "i18n": "ng-xi18n"
    },
    "private": true,
    "dependencies": {
        "@angular/common": "^4.3.5",
        "@angular/compiler": "^4.3.5",
        "@angular/core": "^4.3.5",
        "@angular/forms": "^4.3.5",
        "@angular/http": "^4.3.5",
        "@angular/platform-browser": "^4.3.5",
        "@angular/platform-browser-dynamic": "^4.3.5",
        "@angular/router": "^4.3.5",
        "@types/md5": "^2.1.32",
        "ajv": "^5.2.2",
        "angular2-moment": "^1.6.0",
        "bootstrap": "4.0.0-alpha.5",
        "brace": "^0.10.0",
        "core-js": "^2.5.0",
        "cwl-svg": "^0.0.40",
        "cwlts": "^1.13.36",
        "font-awesome": "^4.7.0",
        "jquery": "^3.2.1",
        "js-yaml": "^3.9.1",
        "markdown-it-lazy-headers": "^0.1.3",
        "md5": "^2.2.1",
        "ngx-pipes": "^1.6.3",
        "rxjs": "^5.4.3",
        "selectize": "^0.12.4",
        "zone.js": "^0.8.16"
    },
    "devDependencies": {
        "@angular/cli": "^1.3.0",
        "@angular/compiler-cli": "^4.3.5",
        "@types/fs-extra": "^4.0.0",
        "@types/jasmine": "^2.5.53",
        "@types/jasminewd2": "^2.0.2",
        "@types/jquery": "^3.2.11",
        "@types/js-yaml": "^3.9.1",
        "@types/markdown-it": "0.0.2",
        "@types/mocha": "^2.2.41",
        "@types/node": "^8.0.22",
        "@types/rimraf": "^2.0.1",
        "@types/selectize": "^0.12.30",
        "@types/webdriverio": "^4.8.4",
        "codelyzer": "^3.1.2",
        "electron": "^1.7.6",
        "electron-builder": "^19.24.1",
        "fs-extra": "^4.0.1",
        "git-changelog": "^1.1.2",
        "glob": "^7.1.1",
        "jasmine-core": "~2.6.2",
        "jasmine-spec-reporter": "~4.1.1",
        "karma": "~1.7.0",
        "karma-chrome-launcher": "~2.1.1",
        "karma-cli": "~1.0.1",
        "karma-coverage-istanbul-reporter": "^1.2.0",
        "karma-jasmine": "~1.1.0",
        "karma-jasmine-html-reporter": "^0.2.2",
        "markdown-it": "^8.3.2",
        "mocha": "^3.5.0",
        "rimraf": "^2.6.1",
        "spectron": "^3.7.2",
        "ts-node": "~2.0.0",
        "tslint": "^5.6.0",
        "typescript": "~2.3.4"
    },
    "engines": {
        "node": ">= 7.4.0",
        "npm": "^5.0.4"
    }
}
