{
    "name": "rabix-composer",
    "version": "1.0.0-rc.3",
    "executorVersion": "1.0.3",
    "executorDownloadURL": "https://github.com/rabix/bunny/releases/download/v1.0.3/rabix-1.0.3.tar.gz",
    "license": "Apache-2.0",
    "homepage": "https://github.com/rabix/composer#readme",
    "description": "Rabix Composer is a desktop IDE for working with Common Workflow Language workflows",
    "angular-cli": {},
    "author": "Seven Bridges",
    "bugs": {
        "url": "https://github.com/rabix/composer/issues"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/rabix/composer.git"
    },
    "scripts": {
        "build": "yarn run compile:electron && yarn run compile:angular && yarn run package",
        "changelog": "./node_modules/.bin/git-changelog",
        "compile:angular": "node --max-old-space-size=4096 ./node_modules/@angular/cli/bin/ng build -prod -aot",
        "compile:electron": "./node_modules/.bin/tsc -p electron && node build-scripts/splash-copy.js",
        "compile:electron:watch": "./node_modules/.bin/tsc -p electron --watch",
        "debug:electron": "./node_modules/.bin/electron ./electron --inspect-brk --dev-mode",
        "lint": "./node_modules/@angular/cli/bin/ng lint",
        "package": "node ./electron-build.js",
        "postinstall": "node build-scripts/bundle-executor.js && cd electron && yarn install",
        "serve": "node --max-old-space-size=4096 ./node_modules/@angular/cli/bin/ng serve -aot",
        "start:electron": "./node_modules/.bin/electron ./electron",
        "test": "yarn run test:electron && yarn run test:angular && yarn run test:spectron",
        "test:angular": "./node_modules/@angular/cli/bin/ng test --watch false",
        "test:angular:watch": "./node_modules/@angular/cli/bin/ng test",
        "test:spectron": "node ./node_modules/mocha/bin/_mocha --timeout 30000 -r ts-node/register --bail --ui bdd ./spectron/**/*.func.ts",
        "test:electron": "./node_modules/electron/cli.js ./electron/node_modules/mocha/bin/_mocha --timeout 15000 -r ts-node/register --recursive --ui bdd 'electron/**/*.spec.ts'",
        "ci": "yarn run build && yarn run test:spectron && yarn run test:electron && yarn run test:angular",
        "i18n": "ng-xi18n",
        "ci:concurrently": "./node_modules/.bin/concurrently --kill-others-on-fail 'yarn run build && yarn run test:spectron' 'yarn run test:electron' 'yarn run test:angular'"
    },
    "private": true,
    "dependencies": {
        "@angular/common": "^5.0.1",
        "@angular/compiler": "^5.0.1",
        "@angular/core": "^5.0.1",
        "@angular/forms": "^5.0.1",
        "@angular/http": "^5.0.1",
        "@angular/platform-browser": "^5.0.1",
        "@angular/platform-browser-dynamic": "^5.0.1",
        "@angular/router": "^5.0.1",
        "@types/md5": "^2.1.32",
        "ajv": "^5.2.2",
        "angular2-moment": "^1.7.0",
        "bootstrap": "4.0.0-alpha.5",
        "brace": "^0.10.0",
        "core-js": "^2.5.0",
        "cwl-svg": "1.1.3",
        "cwlts": "1.15.17",
        "font-awesome": "^4.7.0",
        "jquery": "^3.2.1",
        "js-yaml": "^3.9.1",
        "markdown-it-lazy-headers": "^0.1.3",
        "md5": "^2.2.1",
        "ngx-pipes": "2.0.1",
        "rxjs": "^5.5.0",
        "selectize": "^0.12.4",
        "zone.js": "^0.8.17"
    },
    "devDependencies": {
        "@angular/cli": "^1.5.0",
        "@angular/compiler-cli": "^5.0.1",
        "@types/fs-extra": "^4.0.2",
        "@types/jasmine": "^2.5.54",
        "@types/jasminewd2": "^2.0.2",
        "@types/jquery": "^3.2.12",
        "@types/js-yaml": "^3.9.1",
        "@types/markdown-it": "0.0.4",
        "@types/mocha": "^2.2.44",
        "@types/node": "^8.0.28",
        "@types/rimraf": "^2.0.2",
        "@types/selectize": "^0.12.30",
        "@types/webdriverio": "^4.8.4",
        "archiver": "^2.1.0",
        "codelyzer": "^4.0.1",
        "concurrently": "^3.5.0",
        "electron": "1.7.9",
        "electron-builder": "^19.46.4",
        "fs-extra": "^4.0.1",
        "git-changelog": "^1.1.2",
        "glob": "^7.1.1",
        "jasmine-core": "^2.8.0",
        "jasmine-spec-reporter": "^4.2.1",
        "karma": "~1.7.0",
        "karma-chrome-launcher": "^2.2.0",
        "karma-cli": "~1.0.1",
        "karma-coverage-istanbul-reporter": "^1.2.0",
        "karma-jasmine": "~1.1.0",
        "karma-jasmine-html-reporter": "^0.2.2",
        "markdown-it": "^8.3.2",
        "mocha": "^3.5.0",
        "request": "^2.83.0",
        "rimraf": "^2.6.1",
        "spectron": "^3.7.2",
        "tar": "^4.0.1",
        "ts-node": "^3.3.0",
        "tslint": "^5.7.0",
        "typescript": "~2.4.2",
        "webdriverio": "~4.8.0"
    }
}
